{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "eslint:recommended",
        "plugin:react/recommended",
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 12
    },
    "plugins": [
        "react"
    ],
    "rules": {
        "no-underscore-dangle": 0,
        "no-console": 0,
        "prefer-template": 0,
        "no-multiple-empty-lines": 0,
        "import/newline-after-import": 0,
        "arrow-body-style": 0,
        "no-shadow": 0,
        "no-unused-vars": 0,
        "padded-blocks": 0,
        "comma-dangle": 0,
        "quotes": 0,
        "max-len": 0,
        "indent": 0,
        "object-curly-newline": 0
    }
}
